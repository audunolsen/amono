{
  "name": "playground",
  "version": "0.1.0",
  "description": "Vite & React dev playground",
  "private": true,
  "type": "module",
  "dependencies": {
    "@amono/liaison": "*",
    "@amono/outil": "*",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.23.1"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:preview": "vite build && vite preview --port 3000 --host localdev.retriever-info.com",
    "preview": "vite preview --port 3000 --host localdev.retriever-info.com",
    "pretypecheck": "npm run --include-workspace-root --if-present clean:typescript:root",
    "typecheck": "tsc --build tsconfig.build.json"
  },
  "devDependencies": {
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.2.1",
    "concurrently": "8.2.1",
    "rollup-plugin-visualizer": "5.12.0",
    "vite": "5.3.3"
  }
}
